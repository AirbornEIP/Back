{
    "search.exclude": {
        "node_modules/": true,
        "build/": true,
        "package-lock.json": true,
        ".github/": true,
        ".config/": true,
        ".vscode/": true,
        ".DS_Store": true,
        ".idea": true,
        "reports/": true,
        "src/._ci.js": true,
        ".circleci/": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/build/**": true
    },
    "files.exclude": {
        ".config/": true,
        ".DS_Store": true,
        ".idea": true,
        ".dockerignore": true,
        "reports/": true,
        "src/._ci.js": true,
        "README.md": true
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "eslint.run": "onSave",
    "editor.insertSpaces": true,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "[json]": {
        "editor.tabSize": 2
    }
}